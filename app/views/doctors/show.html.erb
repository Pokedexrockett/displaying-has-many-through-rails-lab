<h1> <%= @doctor.name %> </h1>
<p>
    Department: <%= @doctor.department %>
</p>

Appointments:
    <% @doctor.appointments.each do |appointment| %>
        <%= link_to appointment.patient.name, patient_path(appointment.patient) %> 
        <%= appointment.appointment_datetime.strftime("%B %e, %Y at %H:%M" ) %>
    <% end %>




